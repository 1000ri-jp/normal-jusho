{
  "name": "normal-jusho",
  "version": "0.2.0",
  "description": "TypeScript/JavaScript SDK for the Jusho Japanese address normalization API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "README.ja.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "japanese",
    "address",
    "normalization",
    "geocoding",
    "postal-code",
    "japan",
    "jusho",
    "住所",
    "正規化",
    "郵便番号"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/1000ri-jp/normal-jusho"
  },
  "homepage": "https://jusho.dev",
  "bugs": {
    "url": "https://github.com/1000ri-jp/normal-jusho/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false,
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  }
}
